{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "globals": {
    "document": true,
    "window": true,
    "console": true,
    "Promise": true,
    "XMLHttpRequest": true,
    "SDK_VERSION": true,
    "IS_TEST": true
  },
  "rules": {
    "semi": ["error", "never"],
    "space-before-function-paren": ["error", "always"]
  }
}
