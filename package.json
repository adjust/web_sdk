{
  "name": "@adjustcom/adjust-web-sdk",
  "version": "0.0.2",
  "description": "This is the guide to the Javascript SDK of Adjust™ for web sites or web apps. You can read more about Adjust™ at [adjust.com].",
  "scripts": {
    "build:demo": "webpack --mode production --config webpack.demo.config.js",
    "build:sdk": "webpack --config webpack.sdk.config.js",
    "build:snippet": "terser ./src/sdk/snippet.js -o ./dist/sdk.snippet.min.js -c -m",
    "build": "npm-run-all build:sdk build:demo build:snippet",
    "start": "webpack-dev-server --content-base dist --hot --mode development --config webpack.demo.config.js",
    "lint": "eslint ./src --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "test": "TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watchAll",
    "test:coverage": "TZ=UTC jest --coverage",
    "test:all": "npm-run-all test lint flow",
    "flow": "flow status --quiet",
    "prepublishOnly": "npm run test:all"
  },
  "main": "dist/adjust-latest-test.js",
  "keywords": [],
  "author": "Adjust GmbH",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-flow-comments": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-es6-promise": "^1.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "css-loader": "^1.0.1",
    "es6-promise": "^4.2.8",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "fake-indexeddb": "^2.1.1",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.106.3",
    "flow-webpack-plugin": "^1.2.0",
    "flowtype-loader": "^0.2.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-localstorage-mock": "^2.4.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "terser": "^3.17.0",
    "terser-webpack-plugin": "^1.4.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3"
  },
  "directories": {
    "doc": "docs"
  },
  "files": [
    "dist/adjust-latest-test.js",
    "dist/adjust-latest-test.min.js",
    "docs/**/*",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adjust/web_sdk_dev.git"
  },
  "bugs": {
    "url": "https://github.com/adjust/web_sdk_dev/issues"
  },
  "homepage": "https://github.com/adjust/web_sdk_dev#readme"
}
