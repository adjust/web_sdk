{
  "name": "adjust-sdk",
  "version": "4.1.0",
  "description": "",
  "scripts": {
    "build:demo": "webpack --mode production --config webpack.demo.config.js",
    "build:sdk": "./node_modules/.bin/flow status --quiet && webpack --config webpack.sdk.config.js",
    "build:snippet": "terser ./src/sdk/snippet.js -o ./dist/sdk.snippet.min.js -c -m",
    "build": "npm-run-all build:sdk build:demo build:snippet",
    "start": "webpack-dev-server --content-base dist --hot --mode development --config webpack.demo.config.js",
    "lint": "eslint ./src --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "test": "TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watchAll",
    "test:coverage": "TZ=UTC jest --coverage",
    "test:all": "TZ=UTC npm test && npm run lint",
    "flow:status": "./node_modules/.bin/flow status --quiet"
  },
  "private": true,
  "main": "dist/sdk.js",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-flow-comments": "^7.5.5",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^4.2.0",
    "fake-indexeddb": "^2.1.0",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.106.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.0.0",
    "jest-localstorage-mock": "^2.4.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser": "^3.17.0",
    "terser-webpack-plugin": "^1.2.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
